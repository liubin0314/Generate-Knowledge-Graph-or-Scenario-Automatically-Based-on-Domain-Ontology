###  http://www.semanticweb.org/liubin0314/network#portNumber
:portNumber rdf:type owl:DatatypeProperty ;
            rdfs:domain :服务器 ;
            rdfs:range [ rdf:type rdfs:Datatype ;
                         owl:onDatatype xsd:int ;
                         owl:withRestrictions ( [ xsd:minInclusive "0"^^xsd:int
                                                ]
                                                [ xsd:maxInclusive "65535"^^xsd:int
                                                ]
                                              )
                       ] .


###  http://www.semanticweb.org/liubin0314/network#portProtocol
:portProtocol rdf:type owl:DatatypeProperty ;
              rdfs:domain :服务器 ;
              rdfs:range [ rdf:type rdfs:Datatype ;
                           owl:oneOf [ rdf:type rdf:List ;
                                       rdf:first "IP" ;
                                       rdf:rest [ rdf:type rdf:List ;
                                                  rdf:first "TCP" ;
                                                  rdf:rest rdf:nil
                                                ]
                                     ]
                         ] .


###  http://www.semanticweb.org/liubin0314/network#portState
:portState rdf:type owl:DatatypeProperty ;
           rdfs:domain :服务器 ;
           rdfs:range [ rdf:type rdfs:Datatype ;
                        owl:oneOf [ rdf:type rdf:List ;
                                    rdf:first "closed" ;
                                    rdf:rest [ rdf:type rdf:List ;
                                               rdf:first "open" ;
                                               rdf:rest rdf:nil
                                             ]
                                  ]
                      ] .


